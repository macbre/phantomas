      "gecko": true,
      "offenders": true,
      "unit": "number",
      "module": "domMutations"
    },
    "DOMmutationsRemoves": {
      "desc": "number of <body> node removes",
      "gecko": true,
      "offenders": true,
      "unit": "number",
      "module": "domMutations"
    },
    "DOMmutationsAttributes": {
